        "# Crear un DataFrame con datos adicionales para ejercicios de agrupación\n",
        "data_grupo = {\n",
        "    'Nombre': ['Ana', 'Juan', 'Luis', 'María', 'Ana', 'Juan', 'Luis', 'María'],\n",
        "    'Edad': [23, 29, 34, 25, 23, 29, 34, 25],\n",
        "    'Ciudad': ['Bogotá', 'Medellín', 'Cali', 'Cartagena', 'Bogotá', 'Medellín', 'Cali', 'Cartagena'],\n",
        "    'Producto': ['A', 'B', 'A', 'B', 'A', 'B', 'A', 'B'],\n",
        "    'Ventas': [100, 150, 120, 160, 110, 155, 130, 165]\n",
        "}"
      ],
      "metadata": {
        "id": "ScCWKl5JKmvJ"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Convertir el diccionario en un DataFrame\n",
        "df_grupo = pd.DataFrame(data_grupo)\n",
        "# Mostrar el nuevo DataFrame\n",
        "print(\"\\nNuevo DataFrame para ejercicios de agrupación:\")\n",
        "df_grupo"
      ],
      "metadata": {
        "id": "j7KLpiHRK81S"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Ejercicio 1: Agrupar por columna 'Nombre' y sumar las ventas\n",
        "grupo_nombre = df_grupo.groupby('Nombre')['Ventas'].sum()\n",
        "print(\"\\nSuma total de ventas por 'Nombre':\")\n",
        "print(grupo_nombre)"
      ],
      "metadata": {
        "id": "ZSdamQCNK-Q2"
