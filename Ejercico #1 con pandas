      "source": [
        "# Ejercicio 2: Agrupar por 'Ciudad' y calcular la media de las ventas\n",
        "grupo_ciudad = df_grupo.groupby('Ciudad')['Ventas'].mean()\n",
        "print(\"\\nMedia de ventas por 'Ciudad':\")\n",
        "print(grupo_ciudad)"
